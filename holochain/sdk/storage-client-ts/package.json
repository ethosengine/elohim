{
  "name": "@elohim/storage-client",
  "version": "0.1.0",
  "description": "TypeScript client for elohim-storage sync API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./generated": {
      "types": "./dist/generated/index.d.ts",
      "default": "./dist/generated/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "generate:transformers": "tsx scripts/generate-transformers.ts"
  },
  "keywords": [
    "elohim",
    "storage",
    "crdt",
    "automerge",
    "sync"
  ],
  "author": "Elohim Protocol",
  "license": "AGPL-3.0",
  "dependencies": {
    "@automerge/automerge": "^3.0.0",
    "multiformats": "^13.4.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.0.0",
    "ts-morph": "^21.0.0",
    "@typescript-eslint/parser": "^8.50.1",
    "tsx": "^4.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
