{
  "name": "elohim-dev-proxy",
  "version": "1.0.0",
  "description": "Path-based WebSocket proxy for Holochain development in Eclipse Che",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:local": "node dist/index.js",
    "start:remote": "CONDUCTOR_URL=wss://holochain-dev.elohim.host?apiKey=dev-elohim-auth-2024 node dist/index.js",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "typescript": "~5.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
